#include<stdio.h>
void  main()
{
	char a[50],b[50],c[100];
	int top=0,top1=0,i,j=0;
	printf("ENTER FIRST STRING");
	scanf("%s",a);
	for(i=0;a[i]!='\0';i++)
		++top;
	printf("ENTER SECOND STRING");
	scanf("%s",b);
	for(i=0;b[i]!='\0';i++)
		++top1;
	for(i=0;i<=top;i++)
		c[i]=a[j++];
	j=0;
	for(i=top;i<=top+top1;i++)
		c[i]=b[j++];
	printf("CONCATENATED STRING : %s",c);
}
